<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.diary">

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon2"
        android:label="Scratch Pad"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Forgetpassword1"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name=".ui.login.ForgetPassword"
            android:label="@string/title_activity_forget_password" />
        <activity android:name=".Signup1"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".ui.login.Signup"
            android:screenOrientation="portrait"
            android:label="@string/title_activity_signup" />
        <activity android:name=".tv" />
        <activity android:name=".TextView" />
        <activity android:name=".editor" />
        <activity android:name=".ShowList" />
        <activity android:name=".SplashActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity"/>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>